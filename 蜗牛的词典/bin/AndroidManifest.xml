<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Dictionary"
    android:versionCode="1"
    android:versionName="1.0" >
     <supports-screens
    android:smallScreens="true"
    android:normalScreens="true"
    android:largeScreens="true"
    android:resizeable="true"
    android:anyDensity="true">
</supports-screens>

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
    <instrumentation android:name="android.test.InstrumentationTestRunner" 
        android:targetPackage="com.example.Dictionary">
        
    </instrumentation>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
       
        android:theme="@style/AppTheme" >
       
        
        <activity
            android:name="com.example.Dictionary.MainActivity"
            android:label="@string/app_name" 
           
            android:screenOrientation="nosensor"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
         <activity
            android:name="com.example.Dictionary.SecondActivity"
            android:label="@string/app_name" 
            
            android:screenOrientation="nosensor"
            >
            
        </activity>
         <activity
            android:name="com.example.Dictionary.DetailActivity"
            android:label="@string/app_name" 
            android:theme="@style/Translucent_NoTitle" 
            android:screenOrientation="nosensor"
            >
            
        </activity>
        
         <uses-library android:name="android.test.runner" />
    </application>
	
    <uses-permission android:name="android.permission.RUN_INSTRUMENTATION" />
    
    
</manifest>
